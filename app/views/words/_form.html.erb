<%= form_with model: モデル名, local: true do |f|%>
  <div class="field">
    <%= f.label :title, "カラムの名称" %><br />
    <%= f.text_field :title, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :tug, "タグ" %><br />
    <%= f.text_area :tug, class: :form__text, id:"prototype_catch_copy" %>
  </div>

  <div class="field">
    <%= f.label :text, "投稿記事" %><br />
    <%= f.text_area :text, class: :form__text, id:"prototype_concept" %>
  </div>


  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>